{
  "Local": {
    "Identity": {
      "Apis": {
        "wallet": "Wallet API",
        "walletviews": "Wallet Views API",
        "exchange": "Exchange API"
      },
      "Clients": {
        "js": {
          "id": "js",
          "name": "JS OpenId Client",
          "redirectUris": [
            "/auth/callback",
            "/auth/renewal"
          ],
          "scopes": [
            "wallet",
            "walletviews",
            "exchange"
          ]
        },
        "wallet": {
          "id": "walletswaggerui",
          "name": "Wallet Swagger UI",
          "secret": "secret",
          "redirectUris": [
            "/swagger/o2c.html",
            "/swagger/oauth2-redirect.html"
          ],
          "scopes": [
            "wallet"
          ]
        },
        "walletviews": {
          "id": "walletviewsswaggerui",
          "name": "Wallet Views Swagger UI",
          "secret": "secret",
          "redirectUris": [
            "/swagger/o2c.html",
            "/swagger/oauth2-redirect.html"
          ],
          "scopes": [
            "walletviews"
          ]
        },
        "exchange": {
          "id": "exchangeswaggerui",
          "name": "Exchange Swagger UI",
          "secret": "secret",
          "redirectUris": [
            "/swagger/o2c.html",
            "/swagger/oauth2-redirect.html"
          ],
          "scopes": [
            "exchange"
          ]
        }
      }
    },
    "Sql": {
      "Database": "CM_Services_IdentityDb"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}
